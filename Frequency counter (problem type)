let freq1 = {};
let freq2 = {};
if (arr1.length !== arr2.length) {
  return false;
}

for (let let val of arr1) {
  freq1[val] = (freq1[val] || 0) + 1;
  
for (let val of arr2) {
  freq2[val] = (freq2[val] || 0) + 1;
  
for (let val of freq1) {
  if (freq1[val] !== freq2[val ** 2]) {
    return false;
